{
  "name": "crow-metrics-viz",
  "version": "2.0.0",
  "description": "express plugin that exports and displays crow metrics",
  "keywords": [
    "metrics",
    "crow"
  ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:robey/crow-metrics-viz.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "distclean": "npm run clean && rm -rf node_modules npm-debug.log",
    "prepare": "npm run build",
    "test": "npm run build && mocha -R spec --colors lib/test --timeout 30000"
  },
  "dependencies": {
    "crow-metrics": "^4.0.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "@types/should": "^13.0.0",
    "@types/supertest": "^2.0.4",
    "mocha": "^5.0.4",
    "should": "^13.2.1",
    "source-map-support": "^0.5.3",
    "supertest": "^3.0.0",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "main": "./lib/viz",
  "types": "./lib/vis.d.ts",
  "files": [
    "lib",
    "static"
  ]
}
